{
	"name": "nestjs-fetch",
	"version": "0.1.0",
	"description": "Fetch API as a NestJS provider",
	"author": "Mike Hall",
	"license": "MIT",
	"url": "https://github.com/mikehall314/nestjs-fetch",
	"keywords": [
		"http",
		"fetch",
		"nestjs",
		"request"
	],
	"scripts": {
		"clean": "rimraf dist",
		"format": "prettier --write lib",
		"lint": "eslint lib",
		"lint:staged": "lint-staged",
		"build": "rimraf dist && tsc -p tsconfig.build.json",
		"prepare": "husky",
		"prepublish:npm": "npm run build",
		"publish:npm": "npm publish --access public",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"engineStrict": true,
	"engines": {
		"node": ">=17.5"
	},
	"lint-staged": {
		"**/*.ts": []
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mikehall314/nestjs-fetch"
	},
	"peerDependencies": {
		"@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0"
	},
	"devDependencies": {
		"@nestjs/common": "^11.0.16",
		"@nestjs/core": "^11.0.16",
		"@nestjs/testing": "^11.0.16",
		"@types/jest": "^29.5.11",
		"@types/node": "^17.0.45",
		"@typescript-eslint/parser": "^8.29.1",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-jest": "^28.11.0",
		"husky": "^9.0.7",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.0",
		"neostandard": "^0.12.1",
		"prettier": "^3.5.3",
		"reflect-metadata": "^0.2.2",
		"rimraf": "^6.0.1",
		"rxjs": "^7.8.1",
		"ts-jest": "^29.1.2",
		"typescript": "^5.3.3",
		"typescript-eslint": "^8.29.1"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "lib",
		"testRegex": "\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	},
	"volta": {
		"node": "22.14.0",
		"npm": "11.3.0"
	}
}
